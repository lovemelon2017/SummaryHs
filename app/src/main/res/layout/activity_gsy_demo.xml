<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:paddingLeft="12dp"
        android:text="https://github.com/CarGuo/GSYVideoPlayer"
        android:textColor="#999999"
        android:textSize="12sp" />


    <TextView
        android:id="@+id/gsy_play_list_tv1"
        style="@style/myTextBlue"
        android:text="Video List" />

    <TextView
        android:id="@+id/gsy_play_details_tv"
        style="@style/myTextGreen"
        android:text="Video Details" />

    <TextView
        android:id="@+id/gsy_switch_tv"
        style="@style/myTextBlue"
        android:text="Video Switch" />

    <TextView
        android:id="@+id/gsy_list_auto_tv"
        style="@style/myTextBlue"
        android:text="Video Auto" />

</LinearLayout>